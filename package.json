{
  "name": "jeasx-quickstart",
  "type": "module",
  "scripts": {
    "start": "jeasx start",
    "build": "jeasx build",
    "dev": "concurrently -c auto -n dev,css \"jeasx dev\" \"npm run css -- --watch\"",
    "clean": "jeasx clean",
    "css": "npx @tailwindcss/cli -i ./src/browser/tailwind/tailwind.css -o ./src/browser/tailwind/output.css"
  },
  "dependencies": {
    "jeasx": "2.2.1"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "concurrently": "^9.2.1",
    "tailwindcss": "^4.1.18"
  }
}
